{
    "name": "divineomega/symfony-password-exposed-bundle",
    "description": "Symfony bundle that checks if a password has been exposed in a data breach",
    "keywords": [
        "haveibeenpawnd",
        "password",
        "exposed",
        "pwned",
        "symfony",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "LGPL-3.0-only",
    "support": {
        "issues": "https://github.com/DivineOmega/symfony-password-exposed-bundle/issues",
        "source": "https://github.com/DivineOmega/symfony-password-exposed-bundle/releases",
        "wiki": "https://github.com/DivineOmega/symfony-password-exposed-bundle/wiki"
    },
    "authors": [
        {
            "name": "Nikita Loges",
            "homepage": "https://loges.one",
            "email": "dev@loges.one"
        },
        {
            "name": "Jordan Hall",
            "email": "jordan@hall05.co.uk"
        }
    ],
    "require": {
        "php": "^7.2",

        "symfony/framework-bundle": "^3.4|^4.0",
        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0",
        "symfony/config": "^3.4|^4.0",
        "symfony/yaml": "^3.4|^4.0",
        "symfony/validator": "^3.4|^4.0",
        "symfony/translation": "^3.4|^4.0",
        "symfony/contracts": "^1.0",

        "divineomega/password_exposed": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "DivineOmega\\PasswordExposed\\Symfony\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
